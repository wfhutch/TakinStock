@model IEnumerable<string>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h2>@ViewBag.Title</h2>

<p>Here is a list of items!</p>


@foreach (var item in Model)
{
    <div class="row">
        <div class="col-md-3">
            <div>
                @item
            </div>
        </div>
    </div>
}

